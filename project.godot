; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="GamePlatform"
run/main_scene="res://Start Menu.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Global.gd"
CoinSound="*res://CoinSound.tscn"
LevelCompleteSound="*res://LevelCompleteSound.tscn"
SpikeHitSound="*res://SpikeHitSound.tscn"
MushroomSound="*res://MushroomSound.tscn"
KeySound="*res://KeySound.tscn"
DieSound="*res://DieSound.tscn"
GameMusic="*res://GameMusic.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/borderless=true
window/size/test_width=1280
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/stretch/shrink="1"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
